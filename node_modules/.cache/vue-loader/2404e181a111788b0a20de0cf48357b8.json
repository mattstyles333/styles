      </slot>\n    </label>\n  </div>\n</template>\n<script>\n  export default {\n    name: 'NCheckbox',\n    model: {\n      prop: 'checked'\n    },\n    props: {\n      checked: [Array, Boolean],\n      disabled: Boolean,\n      inline: Boolean,\n      hasError: Boolean\n    },\n    data() {\n      return {\n        cbId: '',\n        touched: false\n      }\n    },\n    computed: {\n      model: {\n        get() {\n          return this.checked\n        },\n        set(check) {\n          if (!this.touched) {\n            this.touched = true\n          }\n          this.$emit('input', check)\n        }\n      },\n      inlineClass() {\n        if (this.inline) {\n          return `form-check-inline`\n        } else {\n          return false\n        }\n      }\n    },\n    created() {\n      this.cbId = Math.random().toString(16).slice(2)\n    }\n  }\n</script>\n<style lang=\"scss\" scoped>\n  @import \"~@/assets/sass/now-ui-kit/variables\";\n  @import \"~@/assets/sass/now-ui-kit/checkboxes-radio\";\n</style>"]}]}